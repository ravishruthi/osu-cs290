<head>
  <link rel="stylesheet" href="style.css" type="text/css">
</head>
<h2>Enter Workout</h2>
<form id="exerciseForm">
  <fieldset>
    <label for="name">Name of Workout: </label>
      <input type="text" name="name" id="name">
    <br />
    <label for="reps">Number of Reps: </label>
      <input type="number" name="reps" id="reps">
    <br />
    <label for="weight">Weight: </label>
      <input type="number" name="weight" id="weight">
    <br />
    <label for="date">Date: </label>
      <input type="date" name="date" id="date">
    <br />
    <label for="unit">Unit: </label>
      <input type="radio" name="unit" value="lbs" checked>
      <label for="lbs">lbs</label>
      <input type="radio" name="unit" value="kg">
      <label for="kg">kg</label>
    <br /><br />
    <input type="submit" id="workoutSubmit" value="Add Workout"/>
  </fieldset>
</form>
<br />
<div></div>
<script src="script.js"></script>
